keywords = {Animals,Computer Simulation,Electromyography,Electrophysiological Phenomena,Macaca mulatta,Male,Models, Neurological,Motor Cortex,Motor Cortex: physiology,Movement,Movement: physiology,Muscle, Skeletal,Muscle, Skeletal: innervation,Muscle, Skeletal: physiology,Nerve Net,Nerve Net: physiology,Neurons,Neurons: physiology},
number = {3},
pages = {440--8},
