journal = {Annual review of neuroscience},
keywords = {Arm,Arm: physiology,Evoked Potentials,Humans,Models,Motor,Motor Cortex,Motor Cortex: physiology,Motor: physiology,Movement,Movement: physiology,Neurological,Nonlinear Dynamics},
pages = {337--359},
